<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.speedsun.wallpaper"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="11" />
    <uses-permission android:name="android.permission.SET_WALLPAPER"></uses-permission>
    <uses-permission android:name="android.permission.BIND_WALLPAPER"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/wallpapers">
        <service
            android:label="@string/speedwallpaper"
            android:name="SpeedWallpaper"
            android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/speedwallpaper" />
        </service>
        <activity
            android:name="com.speedsun.wallpaper.SpeedWallpaperSetting"
            android:exported="true">
        </activity>
    </application>
</manifest>